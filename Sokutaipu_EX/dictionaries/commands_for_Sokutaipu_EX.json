  "n-": "{^}{#Space}{^}","-n": "{^}{#Return}{^}","-Yn": "{^}{#Return}{#Return}{^}","n-n": "{^}{#Space}{#Return}{^}",
  "-*": "{^.}{#Space}{#Return}{^}","-*n": "{^.}{#Space}{#Return}{#Return}{^}",
  "-*k": "{^,}{#Space}{#Return}{^}","-Yk": "{^,}{#Return}{^}",
  "-*t": "{^.}{#Space}{#Return}{^}","-Yt": "{^.}{#Return}{^}",
  "*-*": "{^です.}{#Space}{#Return}{^}","*-*n": "{^です.}{#Space}{#Return}{#Return}{^}",
