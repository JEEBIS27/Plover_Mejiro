  "-Y":"{#Delete}",

                       "-S":"{^}{#Up}{^}",
  "-A":"{^}{#Left}{^}","-N":"{^}{#Down}{^}","-T":"{^}{#Right}{^}",
                "-YS":"{^↑^}",
  "-YA":"{^←^}","-YN":"{^↓^}","-YT":"{^→^}",

  "-I":"{^}{#Control_L(Home)}{^}","-K":"{^}{#Control_L(End)}{^}",

  "TN-KS": "{^?^}","KS-TN": "{^!^}",

