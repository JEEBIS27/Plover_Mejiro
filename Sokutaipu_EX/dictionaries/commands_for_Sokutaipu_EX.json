  "-TA":"{^][^}","-YTA":"{^\\}\\{^}",
  "-KI":"{^)(^}","-YKI":"{^><^}",

  "-TSA":"{^^^}","-YTSA":"{^^^^}{#Left}{^}",
  
  "-KSA":"{^=^}","-YKSA":"{^~^}",
