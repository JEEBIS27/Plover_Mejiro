  "-TKS":"{^々^}","-YTKS":"{^ヶ^}",
  "-KNS":"{^\\^}","-YKNS":"{^＄^}",
  "-TKN":"{^°^}","-YTKN":"{^℃^}",
  "-TNS":"{^%^}","-YTNS":"{^&^}",
  "-TKNS":"{^#^}","-YTKNS":"{^+^}",
  "-NS":"{^:^}","-YNS":"{^;^}",
  "-TK":"{^※^}","-YTK":"{^*^}",
  "-TS":"{^\"^}","-YTS":"{^'^}",
